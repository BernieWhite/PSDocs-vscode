{
    "name": "psdocs-vscode-preview",
    "displayName": "PSDocs",
    "version": "0.0.1",
    "publisher": "bewhite",
    "description": "Adds convenience features for IT Pros using PSDocs.",
    "engines": {
        "vscode": "^1.27.0"
    },
    "license": "SEE LICENSE IN LICENSE",
    "homepage": "https://github.com/BernieWhite/PSDocs-vscode/blob/master/README.md",
    "categories": [
        "Other"
    ],
    "keywords": [
        "PowerShell"
    ],
    "galleryBanner": {
        "color": "#0072c6",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/BernieWhite/PSDocs-vscode.git"
    },
    "bugs": {
        "url": "https://github.com/BernieWhite/PSDocs-vscode/issues"
    },
    "private": true,
    "preview": true,
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "snippets": [
            {
                "language": "powershell",
                "path": "./snippets/PSDocs.json"
            }
        ],
        "commands": [
            {
                "command": "PSDocs.generateDocument",
                "category": "PSDocs",
                "title": "Generate document"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -v && tsc -p ./ && tslint -p ./",
        "compile-watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "vscode": "^1.1.21",
        "vscode-languageclient": "~5.0.0"
    },
    "extensionDependencies": [
        "vscode.powershell"
    ],
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "~10.7.1",
        "mocha": "^4.0.1",
        "tslint": "~5.11.0",
        "typescript": "~3.0.1",
        "vsce": "~1.46.0"
    }
}
